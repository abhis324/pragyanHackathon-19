<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../partials/head %>
</head>
<body>

<header>
    <% include ../partials/header %>
</header>
<br><br><br><br><br><br>

<div class="table-responsive-md">
<table id="msp" class="table">
<caption>MSP Rates across the country</caption>
<thead>
	<tr>
		<th>State Name</th>
		<th>Wheat</th>
		<th>Paddy</th>
		<th>Dal</th>
		<th>Bajra</th>
		<th>Maize</th>
		<th>Gram</th>
		<th>Moong</th>
		<th>Urad</th>
		<th>Soyabean</th>
	</tr>
</thead>
<tbody>
	<% states.forEach(function(state) { %>
		<tr>
			<td><%= state.name %></td>
			<!-- <td><%= crop.rice %></td>
			<td><%= crop.wheat %></td>
			<td><%= crop.sugar %></td>
			<td><%= crop.paddy %></td>
			<td><%= crop.tea %></td>
			 -->
			 <% state.forEach(function(crops){ %>
			 <td><%= crops.rate %></td>

			})
		</tr>
	<% }); %>
</tbody>
</table>
</div>

<footer>
    <% include ../partials/footer %>
</footer>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/js/bootstrap.bundle.min.js"></script>
</body>
</html>
